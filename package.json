{
  "name": "duly-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write  \"./**/*.js\"",
    "lint": "eslint 'src/**/*.js'",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "storybook": "storybook start -p 7007",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "babel-plugin-dotenv-import": "^2.0.0",
    "i18n-js": "^3.1.0",
    "key-mirror-nested": "^1.3.0",
    "moment": "^2.22.2",
    "ramda": "^0.26.0",
    "react": "16.6.1",
    "react-native": "^0.55.4",
    "react-native-i18n": "^2.0.15",
    "react-navigation": "^3.0.0",
    "react-navigation-redux-helpers": "^2.0.8",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-persist": "^5.10.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "type-to-reducer": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.8",
    "@storybook/addon-links": "^4.0.8",
    "@storybook/addons": "^4.0.8",
    "@storybook/react-native": "^4.0.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "jest": "23.6.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "0.49.2",
    "mockdate": "^2.0.2",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.2",
    "react-dom": "16.6.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.6.1",
    "reactotron-react-native": "^2.1.0"
  },
  "lint-staged": {
    "**/*.*": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint",
      "pre-push": "yarn lint && yarn test"
    }
  }
}
